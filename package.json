{
  "name": "vue-network-d3",
  "version": "0.1.2",
  "description": "D3 Force-Directed Graph as Vue Component.",
  "main": "dist/vue-network-d3.umd.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "dev": "vue-cli-service serve",
    "build": "npm run build-component && npm run clean-dist",
    "build-component": "vue-cli-service build --target lib --name vue-network-d3 src/components/Network.vue",
    "clean-dist": "rm dist/demo.html",
    "build-page": "vue-cli-service build --dest page"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChenCyl/vue-network-d3"
  },
  "bugs": {
    "url": "https://github.com/ChenCyl/vue-network-d3/issues"
  },
  "keywords": [
    "vue",
    "d3",
    "d3-force",
    "network",
    "force-directed",
    "graph",
    "component"
  ],
  "dependencies": {
    "@mdi/font": "^7.0.96",
    "d3": "^7.6.1",
    "element-ui": "^2.9.1",
    "vuetify": "^2.6.12"
  },
  "devDependencies": {
    "@vue/cli-service": "^5.0.8",
    "axios": "^1.1.3",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.5.21"
  },
  "author": "ChenCyl",
  "license": "MIT"
}
